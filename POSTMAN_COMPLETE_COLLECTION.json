  "event": [
    {
      "listen": "prerequest",
      "script": {
        "type": "text/javascript",
        "exec": [
          "// Auto-set authorization header if token exists",
          "if (pm.collectionVariables.get('access_token')) {",
          "    pm.request.headers.add({",
          "        key: 'Authorization',",
          "        value: 'Bearer ' + pm.collectionVariables.get('access_token')",
          "    });",
          "    console.log('üîê Authorization header set with token');",
          "}"
        ]
      }
    },
    {
      "listen": "test",
      "script": {
        "type": "text/javascript",
        "exec": [
          "// Global test to check response time",
          "pm.test('‚è±Ô∏è Response time is less than 5000ms', function () {",
          "    pm.expect(pm.response.responseTime).to.be.below(5000);",
          "});",
          "",
          "// Global test to check response has body",
          "pm.test('üìÑ Response has body', function () {",
          "    pm.expect(pm.response.text()).to.not.be.empty;",
          "});",
          "",
          "// Global test to check response is JSON",
          "pm.test('üìã Response is JSON', function () {",
          "    pm.response.to.be.json;",
          "});",
          "",
          "// Log response for debugging",
          "console.log('üìä Response Status:', pm.response.status);",
          "console.log('üìä Response Time:', pm.response.responseTime + 'ms');"
        ]
